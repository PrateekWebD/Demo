<div id="content">
        <!-- section -->
        <div class="section">
          <div class="container py-xl-5">
            <article class="post">
              <span class="meta">{{ article.created_at | date: '%d %B %Y' }}</span>
              <h2>{{ article.title }}</h2>
              {{ article.content }}
            </article>
          </div>
        </div>
</div>

{% for product in collection.cars.products %}
	<h3>{{product.title}}</h3>
	<a href="{{ product.url }}"><img src="{{ product.featured_image | image_url }}" ></a>
{% endfor %}