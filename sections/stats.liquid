<!-- section -->
        <div class="section stats">
          <div class="container">
            <div class="box">
              <div class="row">
                
                {% for block in section.blocks %}
                  {% case block.type %}
                    {% when 'stat-box' %}
                      
                        <div class="col-md-6 column mb-md-0">
                          <div class="inner-wrapper">
                            <span class="values">{{ block.settings.value }}</span>
                            {{ block.settings.content }}
                          </div>
                        </div>
                      
                  {% endcase %}
                {% endfor %}

               
               
              </div>
            </div>
          </div>
        </div>
<!-- /section -->

{% schema %}
  {
    "name": "Stats",
 
	"blocks": [
		{
			"name": "Stat",
			"type": "stat-box",
			"settings": [
				{
					"id": "value",
					"type": "text",
					"label": "Value",
					"default": "Value"
				},
				{
					"id": "content",
					"type": "richtext",
					"label": "Content",
					"default": "<p>Content</p>"
				}
			]
			
		}
	],
	"presets": [
		{
			"name": "Stats"
		}
	]
  }
{% endschema %}
